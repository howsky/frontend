{
  "name": "howsky",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint ./src",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "check": "biome check ./src",
    "check:write": "biome check --write ./src",
    "format": "biome format ./src",
    "format:write": "biome format --write ./src",
    "supabase:login": "supabase login",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "prepare": "husky"
  },
  "engines": {
    "yarn": "Use npm instead of the package manager yarn",
    "pnpm": "Use npm instead of the package manager pnpm",
    "bun": "Use npm instead of the package manager bun"
  },
  "dependencies": {
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.4",
    "dayjs": "^1.11.13",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.453.0",
    "next": "14.2.10",
    "react": "^18",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/next-on-pages": "^1.12.1",
    "@cloudflare/workers-types": "^4.20240718.0",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "supabase": "^1.183.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "vercel": "^37.12.0",
    "wrangler": "^3.81.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["npm run format", "npm run lint"]
  }
}
